.index .wrapper .scroll-product .item{width:270rpx;display:inline-block;margin-right:19rpx;
border-radius: 10rpx;}
.index .wrapper .scroll-product .item:nth-of-type(3n){border-bottom:1rpx solid #ccc;}
.index .wrapper .scroll-product .item:nth-of-type(3n-1){border-bottom:1rpx solid #ccc;}
